<h1>Welcome Admin!</h1>

<h3>Listing users</h3>

<table class="table">
  <thead>
    <tr>
      <th>Username</th>
      <th>Password</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td>FILTERED</td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
